    },
    "w1m_diskspace": {
      "label": "W1m Diskspace",
      "daemon": "onemetre_tcs_diskspace",
      "query_rate": 60,
      "stale_age": 180,
      "parameters": {
        "data_fs_available_bytes": {
          "label": "Available",
          "type": "Latest",
          "display": "DiskBytes",
          "unit": "GiB",
          "warn_limits": [21474836480, 1099511627776],
          "unsafe_limits": [5368709120, 1099511627776]
        }
      }
    },
    "rasa_diskspace": {
      "label": "RASA Diskspace",
      "daemon": "rasa_diskspace",
      "query_rate": 60,
      "stale_age": 180,
      "parameters": {
        "data_fs_available_bytes": {
          "label": "Available",
          "type": "Latest",
          "display": "DiskBytes",
          "unit": "GiB",
          "warn_limits": [214748364800, 2199023255552],
          "unsafe_limits": [5368709120, 2199023255552]
        }
      }
    },
    "tng": {
      "label": "TNG",
      "daemon": "tng_log",
      "query_rate": 180,
      "stale_age": 600,
      "parameters": {
        "dust": {
          "label": "Dust Conc.",
          "type": "Latest",
          "filter_invalid": true,
          "unit": "ug/m\u00B3"
        },
        "seeing": {
          "label": "Seeing.",
          "type": "Latest",
          "filter_invalid": true,
          "unit": "\""
