{
  "daemon": "observatory_environment",
  "log_name": "environmentd",
  "window_length": 1200,
  "control_machines": ["OneMetreDome", "OneMetreTCS", "CLASPTCS", "SWASPTCS", "GOTOServer"],
  "watchers": {
    "w1m_vaisala": {
      "label": "W1m Vaisala",
      "daemon": "onemetre_vaisala",
      "query_rate": 10,
      "stale_age": 30,
      "parameters": {
        "wind_speed": {
          "label": "Wind",
          "unit": "km/h",
          "type": "Range",
          "filter_invalid": true,
          "warn_limits": [0, 30],
          "unsafe_limits": [0, 40]
        },
        "median_wind_speed": {
          "label": "Median Wind",
          "unit": "km/h",
          "type": "Median",
          "median_key": "wind_speed",
          "filter_invalid": true,
          "warn_limits": [0, 15],
          "unsafe_limits": [0, 25]
        },
        "wind_gust": {
          "label": "Wind Gust",
          "unit": "km/h",
          "type": "Range",
          "filter_invalid": true,
          "warn_limits": [0, 30],
          "unsafe_limits": [0, 50]
        },
        "temperature": {
          "label": "Outside Temp.",
          "unit": "\u00B0C",
          "type": "Range",
          "filter_invalid": true,
          "warn_limits": [3, 30],
          "unsafe_limits": [0, 50]
        },
        "relative_humidity": {
          "label": "Outside Hum.",
          "unit": "%RH",
          "type": "Range",
          "filter_invalid": true,
          "warn_limits": [0, 50],
          "unsafe_limits": [0, 75]
        },
        "pressure": {
          "label": "Pressure",
          "unit": "hPa",
          "type": "Latest",
          "filter_invalid": true
        },
        "accumulated_rain": {
          "label": "Accumulated Rain.",
          "unit": "mm",
          "type": "Range",
          "filter_invalid": true,
          "unsafe_limits": [0, 0]
        },
        "dew_point_delta": {
          "label": "Temp. > Dew Pt.",
          "unit": "\u00B0C",
          "type": "Range",
          "filter_invalid": true,
          "warn_limits": [10, 100],
          "unsafe_limits": [5, 100]
        }
      }
    },
    "goto_vaisala": {
