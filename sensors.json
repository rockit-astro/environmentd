    },
    "ephem": {
      "label": "Ephemeris",
      "daemon": "observatory_ephemeris",
      "query_rate": 30,
      "stale_age": 120,
      "parameters": {
        "sun_alt": {
          "label": "Sun Alt.",
          "type": "Latest",
          "unit": "\u00B0",
          "warn_limits": [-90, -10],
          "unsafe_limits": [-90, 2]
        },
        "moon_alt": {
          "label": "Moon Alt.",
          "type": "Latest",
          "unit": "\u00B0"
        },
        "moon_percent_illumination": {
          "label": "Moon Illumination",
          "type": "Latest",
